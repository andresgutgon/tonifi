{
  "name": "tonifi-personal-page",
  "description": "Toni Figuera's personal page",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andresgutgon/tonifi"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.js\"",
    "generateCv": "node scripts/cv/generate.js",
    "cleanCache": "rm -rf .cache/*",
    "cleanDevelop": "npm run cleanCache && gatsby develop",
    "cleanStart": "npm run cleanDevelop",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "npm run cleanCache && npm run generateCv && gatsby build",
    "deploy": "npm run build && gh-pages -d public",
    "jest:watch": "NODE_ENV=test NODE_PATH=scripts jest --watch"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "7.1.6",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "eslint": "6.1.0",
    "eslint-plugin-react": "7.14.2",
    "gh-pages": "2.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "24.8.0",
    "pdfmake": "0.1.50",
    "prettier": "1.14.2"
  },
  "dependencies": {
    "autobind-decorator": "2.3.1",
    "babel-preset-gatsby": "0.2.8",
    "classnames": "2.2.6",
    "formik": "1.5.0",
    "gatsby": "2.13.41",
    "gatsby-image": "2.2.7",
    "gatsby-plugin-i18n": "1.0.1",
    "gatsby-plugin-manifest": "2.0.5",
    "gatsby-plugin-offline": "2.0.11",
    "gatsby-plugin-react-helmet": "3.0.0",
    "gatsby-plugin-sass": "2.0.4",
    "gatsby-plugin-sharp": "2.0.7",
    "gatsby-remark-copy-linked-files": "2.0.11",
    "gatsby-remark-images": "3.0.12",
    "gatsby-source-filesystem": "2.0.8",
    "gatsby-transformer-json": "2.1.5",
    "gatsby-transformer-remark": "2.3.8",
    "gatsby-transformer-sharp": "2.1.4",
    "intl": "1.2.5",
    "node-sass": "4.13.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.0",
    "react-image-lightbox": "github:Resumane/react-image-lightbox#fix-umd-bug",
    "react-intl": "3.0.0-beta.20",
    "react-textarea-autosize": "7.1.0"
  }
}
